{
  "name": "slicklabs/image",
  "license": "MIT",
  "homepage": "https://wefabric.nl/",
  "description": "All Slick image related functionality",
  "type": "library",
  "require": {
    "php": "^7.1.3",
    "ext-exif": "*",
    "intervention/image": "^2.4",
    "spatie/image-optimizer": "^1.1",
    "symfony/console": "^4.2",
    "symfony/filesystem": "^4.2",
    "symfony/finder": "^4.2",
    "tightenco/collect": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Slick\\Image\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true
}
